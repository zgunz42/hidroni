{
  "name": "gatsby-hidroni",
  "private": true,
  "license": "MIT",
  "description": "A simple web shop to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Adi Gunawan <adi_gunawan@live.com>",
  "dependencies": {
    "@loadable/component": "^5.12.0",
    "@mdx-js/mdx": "1.5.7",
    "@mdx-js/react": "1.5.7",
    "classnames": "2.2.6",
    "framer-motion": "1.10.3",
    "gatsby": "^2.19.7",
    "gatsby-image": "^2.2.39",
    "gatsby-plugin-manifest": "^2.2.39",
    "gatsby-plugin-mdx": "1.1.8",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-sass": "2.1.29",
    "gatsby-plugin-sharp": "^2.4.3",
    "gatsby-remark-copy-linked-files": "2.1.37",
    "gatsby-remark-images": "3.1.44",
    "gatsby-remark-relative-images": "0.3.0",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-transformer-sharp": "^2.3.13",
    "gatsby-transformer-yaml": "2.2.25",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "lodash": "4.17.21",
    "node-sass": "latest",
    "patch-package": "^6.2.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-intersection-observer": "8.26.1",
    "slugify": "1.4.0",
    "ventura-slider": "2.9.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-react": "^7.19.0",
    "gatsby-plugin-eslint": "^2.0.8",
    "prettier": "^1.19.1",
    "validate-commit": "^3.4.0"
  },
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,md,html,css,json,yaml}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "autofix": "eslint --fix src/",
    "postinstall": "patch-package",
    "cm": "git-cz",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "validate-commit-msg"
    }
  },
  "lint-staged": {
    "*.{js,jsx,md,html,css,json,yaml}": "yarn format"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
